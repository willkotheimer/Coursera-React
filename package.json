{
  "name": "git-test",
  "version": "1.0.0",
  "description": "This is a test directory to learn git and node",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willkotheimer/bootstrap.git"
  },
  "author": "Will Kotheimer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/willkotheimer/bootstrap/issues"
  },
  "homepage": "https://github.com/willkotheimer/bootstrap#readme",
  "devDependencies": {
    "lite-server": "^2.4.0",
    "node-sass": "^4.9.3",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
